#include<stdio.h>
#include<stdint.h>

int main(){
	uint8_t a,b;  //a and b will store number of elements in array1 and array 2
	printf("Array Swapping Program\n");
	printf("enter number of elements in array1 and array2\n");
		scanf("%d%d",&a,&b);
	uint8_t array1[a],array2[b];


	for(uint8_t i=0;i<a;i++){

		printf("enter element %d of array1:");
		scanf("%d",&array1[i]);
		printf(" %d",array1[i]);
	}
}
